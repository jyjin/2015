@CHARSET "UTF-8";

.player_wrapper {
	position:relative;
	margin: 10px 0px -10px;
	width:100%;
	text-align: center;
}

.player_content{
	width:100%;
	height:100%;
}

.inputcss {
	border-collapse: collapse;
	border: solid 0px Transparent;
	background-color: Transparent;
}

.divInputcss {
	border: solid 3px Transparent;
	background-color: Transparent;
	position: absolute;
	display: none;
	z-index: 1000;
}